Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"recoil", "[", 
    RowBox[{
    "mp_", ",", "mn_", ",", "me_", ",", "gV_", ",", "gA_", ",", "E0_", ",", 
     "E\[Nu]_", ",", "Q2_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["mp", "\[Pi]"], 
