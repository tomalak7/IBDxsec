Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"resultLO", "[", 
    RowBox[{
    "mp_", ",", "mn_", ",", "me_", ",", "gV_", ",", "gA_", ",", "rV2_", ",", 
     "rA2_", ",", "\[Mu]V_", ",", "E\[Nu]_", ",", "Ee_", ",", "hc_", ",", 
     "GF_", ",", "Vud_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"En", "=", 
        RowBox[{"mp", "+", "E\[Nu]", "-", "Ee"}]}], ",", 
       RowBox[{"E0", "=", 
        FractionBox[
         RowBox[{
          SuperscriptBox["mn", "2"], "+", 
          SuperscriptBox["me", "2"], "-", 
          SuperscriptBox["mp", "2"]}], 
         RowBox[{"2", "mn"}]]}], ",", 
       RowBox[{"\[CapitalDelta]", "=", 
        RowBox[{
         RowBox[{"mp", " ", "E\[Nu]"}], "-", 
         RowBox[{"mn", " ", "E0"}]}]}], ",", 
       RowBox[{"s", "=", 
        RowBox[{
         SuperscriptBox["mp", "2"], "+", 
         RowBox[{"2", "mp", " ", "E\[Nu]"}]}]}], ",", 
       RowBox[{"Q2", "=", 
