       RowBox[{"\[Rho]0", "=", 
        FractionBox["me", "Ee"]}]}], "}"}], ",", 
     RowBox[{
      SuperscriptBox["10", "16"], 
      SuperscriptBox["hc", "2"], 
      SuperscriptBox["GF", "2"], 
      SuperscriptBox["Vud", "2"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
