              SuperscriptBox["E\[Nu]", "2"]}]], 
            RowBox[{"(", 
             RowBox[{
              SuperscriptBox["gV", "2"], "-", 
              SuperscriptBox["gA", "2"]}], ")"}]}]}], ")"}], 
         FractionBox["mp", "\[Pi]"]}], "+", 
        RowBox[{"recoil", "[", 
         RowBox[{
         "mp", ",", "mn", ",", "me", ",", "gV", ",", "gA", ",", "E0", ",", 
          "E\[Nu]", ",", "Q2"}], "]"}], "+", 
        RowBox[{"WM", "[", 
