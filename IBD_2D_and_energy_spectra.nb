       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["gV", "2"], "+", 
         SuperscriptBox["gA", "2"]}], ")"}]}], "-", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["gV", "2"], "-", 
